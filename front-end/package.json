{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.16.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.12",
    "formik": "^1.5.2",
    "jest-junit": "^6.3.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.1",
