      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {},
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/antd/es/notification/style/index.js"
    ]
